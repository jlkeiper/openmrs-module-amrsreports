<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.amrsreports.model">
	<class name="ARTRegimenDrug" table="amrsreports_art_regimen_drug">

        <id column="art_regimen_drug_id" name="id" type="int" unsaved-value="0">
			<generator class="native"/>
		</id>
		<discriminator column="art_regimen_drug_id" insert="false"/>

        <property name="name" type="java.lang.String" not-null="true"/>
        <property name="description" type="java.lang.String" />

        <set name="drugs" table="amrsreports_art_regimen_drug_drug" cascade="delete">
            <key column="art_regimen_drug_id"/>
            <many-to-many column="drug_id" unique="true" class="org.openmrs.Drug"/>
        </set>

        <many-to-one name="creator" class="org.openmrs.User" not-null="true">
            <column name="creator" />
        </many-to-one>
        <property name="dateCreated" type="java.util.Date" column="date_created" not-null="true" length="19" />

        <many-to-one name="changedBy" class="org.openmrs.User" not-null="true">
            <column name="changed_by" />
        </many-to-one>
        <property name="dateChanged" type="java.util.Date" column="date_changed" length="19" />

        <many-to-one name="retiredBy" class="org.openmrs.User" column="retired_by" />
        <property name="dateRetired" type="java.util.Date" column="date_retired" length="19" />
        <property name="retireReason" type="java.lang.String" column="retire_reason" length="255" />
        <property name="retired" type="boolean" column="retired" length="1" not-null="true" />

        <property length="38" name="uuid" type="java.lang.String" unique="true"/>
	</class>
</hibernate-mapping>
